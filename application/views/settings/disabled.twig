{% extends '_layouts/template.twig' %}

{% block content_header %}
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <ol class="breadcrumb">
      <li><a href="/"><i class="fa fa-dashboard"></i> Home</a></li>
      <li class="active">Disabled</li>
    </ol>
  </section>
{% endblock %}

{% block content %}
  <!-- Main content -->
  <section class="content">
    <div class="error-page">
      <h2 class="headline text-yellow">OOPS!&nbsp;</h2>
      <div class="error-content">
        <h3><i class="fa fa-warning text-yellow"></i> This feature is currently disabled.</h3>
        <p>
          <big>
            If this is supposed to be enabled, please contact the JCRC!
          </big>
        </p>
      </div><!-- /.error-content -->
    </div><!-- /.error-page -->
  </section><!-- /.content -->
{% endblock %}
